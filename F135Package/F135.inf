[Manufacturer]
%KN%   = KNMfg,NTAMD64

[KNMfg.NTAMD64]
; Firmware Loader Driver
%KN.DeviceDesc0% = WDGTLDR.NT, USB\VID_4705&PID_0211&REV_0000
