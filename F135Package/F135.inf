%KN.DeviceDesc17% = WDGTLDR.NT, USB\VID_0F05&PID_F235&REV_AA07
%KN.DeviceDesc17% = WDGTLDR.NT, USB\VID_0F05&PID_F235&REV_::07
%KN.DeviceDesc18% = WDGTLDR.NT, USB\VID_0F05&PID_F235&REV_AA08
%KN.DeviceDesc18% = WDGTLDR.NT, USB\VID_0F05&PID_F235&REV_::08

;
; Source file information
;

[SourceDisksFiles]
F135USB.sys=1
F135Loader.sys=1
PknInit.hex=1
Pakon5.hex=1
Pakon7.hex=1
Pakon8.hex=1

[SourceDisksNames]
1=%DiskId1%,F235Disk.tag

[DestinationDirs]
F135USB.CopyFiles.Ext = 10,System32\Drivers
WDGTLDR.CopyFiles.Ext = 10,System32\Drivers
WDGTLDR.CopyFiles.Fw  = 10,System32\F235Firmware

[F135USB.NT]
AddReg=F135USB.AddReg
CopyFiles=F135USB.CopyFiles.Ext

[F135USB.CopyFiles.Ext]
F135USB.sys

[F135USB.NT.Services]
AddService = F135USB, 0x00000002 , F135USB.AddService

[F135USB.AddReg]
HKR,,DevLoader,,*ntkern
HKR,,NTMPDriver,,F135USB.sys

[F135USB.AddService]
DisplayName    = %ServiceNameF135%
ServiceType    = 1                  ; SERVICE_KERNEL_DRIVER
StartType      = 3                  ; SERVICE_AUTO_START
ErrorControl   = 1                  ; SERVICE_ERROR_IGNORE
LoadOrderGroup = Base
ServiceBinary  = %10%\system32\drivers\F135USB.sys

[WDGTLDR.NT]
AddReg=WDGTLDR.AddReg
CopyFiles=WDGTLDR.CopyFiles.Ext,WDGTLDR.CopyFiles.Fw

[WDGTLDR.CopyFiles.Ext]
F135Loader.sys

[WDGTLDR.CopyFiles.Fw]
PknInit.hex
Pakon5.hex
Pakon7.hex
Pakon8.hex

[WDGTLDR.NT.Services]
AddService = WDGTLDR, 0x00000002 , WDGTLDR.AddService

[WDGTLDR.AddReg]
HKR,,DevLoader,,*ntkern
HKR,,NTMPDriver,,F135Loader.sys

[WDGTLDR.AddServiceReg]
HKR,,FirmwareDirectory,,\SystemRoot\System32\F235Firmware
HKR,,F235_AA05,,Pakon5.hex
HKR,,F235_AA07,,Pakon7.hex
HKR,,F235_AA08,,Pakon8.hex

[WDGTLDR.AddService]
DisplayName    = %ServiceName5%
AddReg         = WDGTLDR.AddServiceReg
ServiceType    = 1                  ; SERVICE_KERNEL_DRIVER
StartType      = 3                  ; SERVICE_AUTO_START
ErrorControl   = 1                  ; SERVICE_ERROR_IGNORE
LoadOrderGroup = Base
ServiceBinary  = %10%\system32\drivers\F135Loader.sys

